    <table align="center">
        <tr><td><input type="hidden" name="on0" value="Número de caracteres">Número de caracteres</td></tr>
        <tr>
            <td>
                <select name="os0">
                    <option value="100000 caracteres">100000 caracteres €5,00 EUR</option>
                    <option value="200000 caracteres">200000 caracteres €10,00 EUR</option>
                    <option value="600000 caracteres">600000 caracteres €25,00 EUR</option>
                    <option value="1500000 caracteres">1500000 caracteres €50,00 EUR</option>
                </select>
            </td>
        </tr>
    </table>
    <input type="hidden" name="currency_code" value="EUR">
