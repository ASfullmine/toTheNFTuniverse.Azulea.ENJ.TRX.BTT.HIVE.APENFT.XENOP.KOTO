    <input type="hidden" name="hosted_button_id" value="N4HZUPEJFHQ8Q">
    <table align="center">
        <tr><td><input type="hidden" name="on0" value="Número de caracteres">Number of characters</td></tr>
        <tr>
            <td>
                <select name="os0">
                    <option value="100000 caracteres">100000 characters €5,00 EUR</option>
                    <option value="200000 caracteres">200000 characters €10,00 EUR</option>
                    <option value="600000 caracteres">600000 characters €25,00 EUR</option>
                    <option value="1500000 caracteres">1500000 characters €50,00 EUR</option>
                </select>
            </td>
        </tr>
    </table>
    <input type="hidden" name="currency_code" value="EUR">
    <input name="custom" value="@(((Guid)Model).ToString())" type="hidden" />
