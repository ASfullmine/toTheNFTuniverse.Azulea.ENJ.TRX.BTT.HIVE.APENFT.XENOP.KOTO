<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 666way.com</title>
    <meta name="description" content="666way.com - 1ª Edición - de Murcia a Holanda andando">
    <meta name="language" content="es-ES">
    <meta name="author" content="Pablo Pérez Sánchez">
    <meta name="robots" content="index, follow">
    <meta property="og:title" content="666way.com" />
    <meta property="og:description" content="666way.com - 1ª Edición - de Murcia a Holanda andando" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="http://www.666way.com" />
    <meta property="og:image" content="http://www.666way.com/imgs/666way_negro_logo.jpg" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body style="text-align:center;color:white;background-color:black;">
    <table width="100%" style="width:100%;">
        <tr>
            <td style="width:20%;vertical-align:top;">
                <a href="~/Home/Index" style="color:red;font-size:xx-large;">
                    <img height="100" width="100" style="height:100px;width:100px;" src="~/imgs/666way_negro_logo.jpg" alt="666way Logo" />
                </a>
            </td>
            <td style="width:60%;">
                <a href="~/Home/Index" style="color:red;font-size:xx-large;">666way.com</a><br />
                <a href="~/Home/Musica" style="color:red;font-size:xx-large;">&Delta;</a>
            </td>
            <td style="width:20%;vertical-align:top;">
                <a href="~/Home/Index" style="color:red;font-size:xx-large;">
                    <img height="100" width="100" style="height:100px;width:100px;" src="~/imgs/666way_negro_logo.jpg" alt="666way Logo" />
                </a>
            </td>
        </tr>
        <tr>
            <td style="width:20%;vertical-align:top;"></td>
            <td style="width:60%;"></td>
            <td style="width:20%;vertical-align:top;"></td>
        </tr>
        <tr>
            <td style="width:20%;vertical-align:top;">
                <a href="~/Home/PublicidadI" style="color:white;">Publicidad +</a><br />

                @if (ViewBag.publiI is List<_666way.Models.publicidadModel>)
                {
                    List<_666way.Models.publicidadModel> publiI = (List<_666way.Models.publicidadModel>)ViewBag.publiI;
                    Int32 i = 0;
                    foreach (_666way.Models.publicidadModel p in publiI)
                    {
                        i++;
                        <br />
                        <br />
                        <a target="_blank" href="@p.UrlD"><img height="100" width="100" style="height:100px;width:100px;" src="@p.UrlImage" alt="666way - Sponsor Izq @i" /></a>
                    }
                }

            </td>
            <td style="width:60%;">@RenderBody()</td>
            <td style="width:20%;vertical-align:top;">
                <a href="~/Home/PublicidadD" style="color:white;">Publicidad +</a><br />
                @if (ViewBag.publiD is List<_666way.Models.publicidadModel>)
                {
                    List<_666way.Models.publicidadModel> publiD = (List<_666way.Models.publicidadModel>)ViewBag.publiD;
                    Int32 i = 0;
                    foreach (_666way.Models.publicidadModel p in publiD)
                    {
                        i++;
                        <br />
                        <br />
                        <a target="_blank" href="@p.UrlD"><img height="100" width="100" style="height:100px;width:100px;" src="@p.UrlImage" alt="666way - Sponsor Der @i" /></a>
                    }
                }
            </td>
        </tr>
    </table>
    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year - 666way.com</p>
    </footer>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
